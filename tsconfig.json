{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "baseUrl": "./",
    "module": "ES2015",
    "moduleResolution": "Node",
    "noImplicitAny": true,
    "outDir": "dist/",
    "paths": {
      "*": [
        "src/types/*",
        "*"
      ]
    },
    "target": "ES5"
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}